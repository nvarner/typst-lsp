{
    "comments": {
        "lineComment": "//",
        "blockComment": [ "/*", "*/" ]
    },
    "brackets": [
        [ "[", "]" ],
        [ "{", "}" ],
        [ "(", ")" ]
    ],
    "autoClosingPairs": [
        [ "[", "]" ],
        [ "{", "}" ],
        [ "(", ")" ],
        {
            "open": "\"",
            "close": "\"",
            "notIn": [ "string" ]
        },
        {
            "open": "$",
            "close": "$",
            "notIn": [ "string" ]
        }
    ],
    "autoCloseBefore": "$ \n\t",
    "surroundingPairs": [
        [ "[", "]" ],
        [ "{", "}" ],
        [ "(", ")" ],
        [ "\"", "\"" ],
        [ "*", "*" ],
        [ "_", "_" ],
        [ "`", "`" ],
        [ "$", "$" ]
    ]
}
